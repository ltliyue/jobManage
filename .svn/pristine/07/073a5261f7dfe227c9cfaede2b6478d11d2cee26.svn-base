log4j.logger.java.sql=debug,FILE,root
log4j.category.com.guagua=debug,FILE,root
log4j.category.com.opensymphony=WARN,FILE,root
log4j.category.org=WARN,FILE,root

log4j.appender.FILE=org.apache.log4j.DailyRollingFileAppender
log4j.appender.FILE.File=../crawlLog/out.log
log4j.appender.FILE.DatePattern='.'yyyy-MM-dd
log4j.appender.FILE.layout=org.apache.log4j.PatternLayout
log4j.appender.FILE.layout.ConversionPattern=%-5p %d{HH:mm:ss.SSS} %6.6t %-20C{1}:%4L %m%n

log4j.appender.root=org.apache.log4j.ConsoleAppender 
log4j.appender.root.layout=org.apache.log4j.PatternLayout
log4j.appender.root.layout.ConversionPattern=%-5p %d{HH:mm:ss.SSS} %6.6t %l %m%n
